/* themes/midnight_ember_theme.qss */

/* --- Optimized Properties for Midnight Ember Theme --- */
--main-window-bg: #080A0F;
--label-color: #E0E5EB;
--groupbox-font-size: 10pt;
--groupbox-font-weight: bold;
--groupbox-bg: #0F121C;
--groupbox-border-width: 1;
--groupbox-border-style: solid;
--groupbox-border-color: #1A1D29;
--groupbox-title-color: #7B8499;
--groupbox-border-radius: 8;
--tabwidget-pane-bg: #0F121C;
--tabwidget-pane-border-width: 1;
--tabwidget-pane-border-style: solid;
--tabwidget-pane-border-color: #1A1D29;
--tab-bg-normal: #080A0F;
--tab-color-normal: #7B8499;
--tab-bg-selected: #0F121C;
--tab-color-selected: #E0E5EB;
--combobox-bg: #1A1D29;
--combobox-color: #E0E5EB;
--combobox-border: "1px solid #1A1D29";
--combobox-border-color: #1A1D29;
--combobox-selection-bg: #E55C2F; /* Ember Accent */
--button-bg-normal: #BF401D; /* Ember Accent */
--button-color-normal: #FFFFFF;
--button-border-color: #BF401D;
--button-hover-bg: #E55C2F;
--button-pressed-bg: #A03417;
--button-border-radius: 4;
--lineedit-bg: #1A1D29;
--lineedit-color: #E0E5EB;
--lineedit-border: "1px solid #1A1D29";
--statusbar-bg: #080A0F;
--statusbar-color: #7B8499;
--info-status-color: #28A745;
--warning-status-color: #FFC107;
--error-status-color: #FF0000;
--gauge-border-width: 1;
--gauge-border-style: solid;
--gauge-border-color: #1A1D29;
--statusbar-warning-bg: #FFC107;
--statusbar-warning-fg: #080A0F;
--statusbar-critical-bg: #FF0000;
--statusbar-critical-fg: #FFFFFF;

/* --- Metric Symbols --- */
symbol_temperature: 'üå°Ô∏è';
symbol_humidity: 'üíß';
symbol_pressure: 'üí®';
symbol_altitude: '‚õ∞Ô∏è';
symbol_light: 'üí°';
symbol_default: '‚ö´';
symbol_color: '#BF401D';

/* Matplotlib Plot Colors */
matplotlib_border_normal: 1px solid #1A1D29;
matplotlib_container_bg: #080A0F;
plot_font_size: 10;
plot_font_family: 'Inter';
matplotlib_figure_facecolor: #080A0F;
matplotlib_axes_facecolor: #0F121C;
matplotlib_edgecolor: #1A1D29;
matplotlib_label_color: #7B8499;
matplotlib_tick_color: #3A3F4C;
matplotlib_title_color: #7B8499;
matplotlib_grid_color: #1A1D29;
matplotlib_legend_facecolor: #1A1D29;
matplotlib_legend_label_color: #E0E5EB;
matplotlib_line_colors: ["#BF401D", "#28A745", "#FFC107", "#FF0000", "#7B8499", "#A78BFA", "#E0E5EB"];
plot_threshold_low_color: #FFC107;
plot_threshold_high_color: #FF0000;
plot_na_text_color: #3A3F4C;

