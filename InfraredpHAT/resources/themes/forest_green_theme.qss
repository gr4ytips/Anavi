--tabwidget-pane-border-style: solid;
--tabwidget-pane-border-color: #4A6C5B;
--tab-bg-normal: #1A2421;
--tab-color-normal: #A5D6A7;
--tab-bg-selected: #2A3F33;
--tab-color-selected: #E8F5E9;
--combobox-bg: #334E44;
