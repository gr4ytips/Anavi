--lineedit-border: "1px solid #4A6C5B";
--statusbar-bg: #1A2421;
--statusbar-color: #A5D6A7;
--info-status-color: #26A69A;
--warning-status-color: #F59E0B;
--error-status-color: #F43F5E;
--gauge-border-width: 1;
