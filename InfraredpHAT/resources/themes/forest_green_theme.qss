/* Matplotlib Plot Colors */
matplotlib_border_normal: 1px solid #4A6C5B;
matplotlib_container_bg: #1A2421;
plot_font_size: 10;
plot_font_family: 'Inter';
matplotlib_figure_facecolor: #1A2421;
matplotlib_axes_facecolor: #2A3F33;
matplotlib_edgecolor: #4A6C5B;
matplotlib_label_color: #A5D6A7;
