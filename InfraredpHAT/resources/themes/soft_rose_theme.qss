/* themes/soft_rose_theme.qss */

/* --- Optimized Properties for Soft Rose Theme --- */
--main-window-bg: #2E1B1D;
--label-color: #FFEFF0;
--groupbox-font-size: 10pt;
--groupbox-font-weight: bold;
--groupbox-bg: #40292B;
--groupbox-border-width: 1;
--groupbox-border-style: solid;
--groupbox-border-color: #6D4D50;
--groupbox-title-color: #C97D81;
--groupbox-border-radius: 8;
--tabwidget-pane-bg: #40292B;
--tabwidget-pane-border-width: 1;
--tabwidget-pane-border-style: solid;
--tabwidget-pane-border-color: #6D4D50;
--tab-bg-normal: #2E1B1D;
--tab-color-normal: #C97D81;
--tab-bg-selected: #40292B;
--tab-color-selected: #FFEFF0;
--combobox-bg: #513639;
--combobox-color: #FFEFF0;
--combobox-border: "1px solid #6D4D50";
--combobox-border-color: #6D4D50;
--combobox-selection-bg: #E91E63;
--button-bg-normal: #E91E63;
--button-color-normal: #FFFFFF;
--button-border-color: #E91E63;
--button-hover-bg: #EC407A;
--button-pressed-bg: #C2185B;
--button-border-radius: 4;
--lineedit-bg: #513639;
--lineedit-color: #FFEFF0;
--lineedit-border: "1px solid #6D4D50";
--statusbar-bg: #2E1B1D;
--statusbar-color: #C97D81;
--info-status-color: #4CAF50;
--warning-status-color: #FFC107;
--error-status-color: #EF5350;
--gauge-border-width: 1;
--gauge-border-style: solid;
--gauge-border-color: #6D4D50;
--statusbar-warning-bg: #FFC107;
--statusbar-warning-fg: #2E1B1D;
--statusbar-critical-bg: #EF5350;
--statusbar-critical-fg: #FFFFFF;

/* --- Metric Symbols --- */
symbol_temperature: 'üå°Ô∏è';
symbol_humidity: 'üíß';
symbol_pressure: 'üí®';
symbol_altitude: '‚õ∞Ô∏è';
symbol_light: 'üí°';
symbol_default: '‚ö´';
symbol_color: '#E91E63';

/* Matplotlib Plot Colors */
matplotlib_border_normal: 1px solid #6D4D50;
matplotlib_container_bg: #2E1B1D;
plot_font_size: 10;
plot_font_family: 'Inter';
matplotlib_figure_facecolor: #2E1B1D;
