--groupbox-title-color: #80DEEA;
--groupbox-border-radius: 8;
--tabwidget-pane-bg: #0A1A1F;
--tabwidget-pane-border-width: 1;
--tabwidget-pane-border-style: solid;
--tabwidget-pane-border-color: #33B4C4;
--tab-bg-normal: #000000;
--tab-color-normal: #80DEEA;
