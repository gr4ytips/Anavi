--statusbar-bg: #000000;
--statusbar-color: #80DEEA;
--info-status-color: #40C4FF;
--warning-status-color: #FFEE58;
--error-status-color: #FF4081;
--gauge-border-width: 1;
--gauge-border-style: solid;
--gauge-border-color: #33B4C4;
--statusbar-warning-bg: #FFEE58;
--statusbar-warning-fg: #000000;
--statusbar-critical-bg: #FF4081;
--statusbar-critical-fg: #000000;

/* Matplotlib Plot Colors */
matplotlib_border_normal: 1px solid #33B4C4;
matplotlib_container_bg: #000000;
plot_font_size: 10;
plot_font_family: 'Inter';
matplotlib_figure_facecolor: #000000;
matplotlib_axes_facecolor: #0A1A1F;
matplotlib_edgecolor: #33B4C4;
matplotlib_label_color: #80DEEA;
matplotlib_tick_color: #78A3AD;
